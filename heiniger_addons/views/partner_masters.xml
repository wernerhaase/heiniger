<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="orchid_buss_type_view_form" model="ir.ui.view">
            <field name="name">Business Entity Type</field>
            <field name="model">orchid.buss.type</field>
            <field name="arch" type="xml">
                <form string="Business Entity Type">
                    <sheet>
                        <group>
                            <group>
                                <field name="name" required="1"/>
                                <field name="code"/>
                            </group>
                            <group/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="orchid_buss_type_view_tree" model="ir.ui.view">
            <field name="name">Business Entity Type</field>
            <field name="model">orchid.buss.type</field>
            <field name="arch" type="xml">
                <tree string="Business Entity Type">
                    <field name="name"/>
                    <field name="code"/>
                </tree>
            </field>
        </record>

        <record id="action_orchid_buss_type" model="ir.actions.act_window">
            <field name="name">Business Entity Type</field>
            <field name="res_model">orchid.buss.type</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="orchid_buss_type_view_tree"/>
        </record>



