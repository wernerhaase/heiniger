        <record model="ir.ui.view" id="orchid_product_brand_tree_view">
            <field name="name">orchid.product.brand.tree</field>
            <field name="model">orchid.product.brand</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="tree_string">
                    <field name="name" />
                    <field name="code" />
                </tree>
            </field>
        </record>

